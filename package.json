{
  "name": "macro-mcp",
  "version": "2.0.0",
  "description": "MCP server for macro tracking with Supabase authentication and OAuth - Express.js + Docker",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "docker:build": "docker build -t macro-mcp .",
    "docker:run": "docker run -p 3000:3000 --env-file .env macro-mcp",
    "docker:compose": "docker-compose up --build",
    "inspector": "npx @modelcontextprotocol/inspector --config mcp-config.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mcp",
    "nutrition",
    "macronutrients",
    "macro-tracking",
    "calories",
    "supabase",
    "oauth",
    "express",
    "model-context-protocol"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@supabase/supabase-js": "^2.48.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.17.2",
    "commander": "^14.0.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
